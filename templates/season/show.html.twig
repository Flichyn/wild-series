{% extends 'base.html.twig' %}
{% block title %}{{ 'season.season'|trans }}{% endblock %}
{% block body %}
    <h1>{{ 'season.season'|trans }}</h1>
    <table class="table">
        <tbody>
            <tr>
                <th>ID</th>
                <td>{{ season.id }}</td>
            </tr>
            <tr>
                <th>{{ 'season.number'|trans }}</th>
                <td>{{ season.number }}</td>
            </tr>
            <tr>
                <th>{{ 'season.year'|trans }}</th>
                <td>{{ season.year }}</td>
            </tr>
            <tr>
                <th>Description</th>
                <td>{{ season.description }}</td>
            </tr>
        </tbody>
    </table>
    <a href="{{ path('season_index') }}">{{ 'back.seasonList'|trans }}</a>
    <a href="{{ path('season_edit', {'id': season.id}) }}">{{ 'season.edit'|trans }}</a>
    {{ include('season/_delete.html.twig') }}
{% endblock %}
